<%- include('./partials/navbar.ejs'); %>

<div class="text-center">
	<h1>Tinnies History</h1>
	<table class="table">
		<thead>
			<tr>
				<th>Date</th>
				<th>Drank or Added</th>
				<th>How many</th>
			</tr>
		</thead>
		<tbody>
			<% history.forEach(row => { %> <% let date = new Date(row.datetime * 1000)
			%>
			<tr>
				<td><%= date %> <%= %></td>
				<td>
					<%= parseInt(row.pos_or_neg) + 1 ? "Drank" : "Added" %>
				</td>
				<td><%= row.history_tinnies_count %></td>
			</tr>
			<% }) %>
		</tbody>
	</table>
	<p><%= history[0].datetime %></p>
</div>
