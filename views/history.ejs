<%- include('./partials/navbar.ejs'); %>

<div class="text-center">
	<h1>Tinnies History</h1>
	<table class="table">
		<thead>
			<tr>
				<th>Date</th>
				<th>Drank or Added</th>
				<th>How many</th>
			</tr>
		</thead>
		<tbody>
			<% history.forEach(row => { %> <% let date = new Date(row.datetime * 1000)
			%>
			<tr>
				<%= date %> <%= %>
			</tr>
			<% }) %>
		</tbody>
	</table>
	<p><%= history[0].datetime %></p>
</div>
